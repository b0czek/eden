{
  "name": "@edenapp/eden",
  "version": "0.0.0",
  "private": true,
  "description": "Eden Desktop Environment",
  "scripts": {
    "sdk:build": "cd packages/sdk && pnpm run build",
    "scripts:build": "cd packages/scripts && pnpm run build",
    "genesis:build": "cd genesis && pnpm run build",
    "clean": "rm -rf dist packages/*/dist genesis/dist",
    "dev": "cd packages/example && pnpm dev",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm run build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8"
  }
}
