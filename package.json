{
  "name": "@edenapp/eden",
  "version": "0.0.0",
  "private": true,
  "description": "Eden Desktop Environment",
  "scripts": {
    "build": "pnpm -r --filter=!@edenapp/example run build && pnpm install",
    "clean": "rm -rf dist packages/*/dist",
    "dev": "cd packages/example && pnpm dev",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm run build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8"
  }
}
