{
  "id": "com.eden.example.calculator",
  "name": "Calculator",
  "version": "1.0.0",
  "description": "A simple calculator app",
  "author": "Eden Team",
  "frontend": {
    "entry": "index.html"
  },
  "backend": {
    "entry": "backend.js"
  },
  "window": {
    "mode": "floating",
    "defaultSize": {
      "width": 400,
      "height": 600
    },
    "minSize": {
      "width": 300,
      "height": 400
    },
    "maxSize": {
      "width": 600,
      "height": 800
    },
    "resizable": true,
    "movable": true,
    "showTitle": false
  },
  "permissions": {}
}
