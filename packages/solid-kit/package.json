{
  "name": "@edenapp/solid-kit",
  "version": "0.5.1",
  "description": "High-level SolidJS UI primitives for Eden apps",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./dialogs": {
      "types": "./src/dialogs.ts",
      "import": "./src/dialogs.ts",
      "default": "./src/dialogs.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "solid-js": "^1.9.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "keywords": [
    "eden",
    "solid",
    "dialog",
    "renderer"
  ],
  "author": "Dariusz Majnert",
  "license": "MIT"
}
