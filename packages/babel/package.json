{
  "name": "@edenapp/babel",
  "version": "0.5.1",
  "description": "Shared i18n logic and translations for Eden apps",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./solid": {
      "types": "./dist/solid.d.ts",
      "import": "./dist/solid.js",
      "default": "./dist/solid.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "default": "./dist/types.js"
    },
    "./generated/i18n": {
      "types": "./dist/generated/i18n.d.ts",
      "import": "./dist/generated/i18n.js",
      "default": "./dist/generated/i18n.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "i18next": "^23.7.11"
  },
  "peerDependencies": {
    "solid-js": "^1.9.0"
  },
  "devDependencies": {
    "@edenapp/types": "workspace:*",
    "typescript": "^5.3.0"
  }
}
